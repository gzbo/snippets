{% for variant in product.variants %}
  {% if variant.metafields.bundles_app.content.size > 0 %}
      {% assign lines = variant.metafields.bundles_app.content.value %}
      <ul>
      {% for line in lines %}
        {% for fields in line %}
          	{%if fields[0] == 'title'%}
        		{% assign title = fields[1] %}
        	{%endif%}
        	{%if fields[0] == 'sku'%}
        		{% assign sku = fields[1] %}
        	{%endif%}
        	{%if fields[0] == 'quantity'%}
        		{% assign quantity = fields[1] %}
        	{%endif%}
        {% endfor %}
        <li>{{quantity}}x {{title}} ({{sku}})</li>
      {% endfor %}
      </ul>
  {% endif %}
{% endfor %}
